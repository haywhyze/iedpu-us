{
  "name": "with-firebase-hosting",
  "version": "4.0.1",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next \"src/app/\"",
    "preserve": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "predeploy": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build-app": "next build \"src/app/\"",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@date-io/date-fns": "^1.3.13",
    "@emotion/core": "^10.0.28",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "chartist": "^0.11.4",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "date-fns": "^2.14.0",
    "firebase": "^7.14.6",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.2",
    "firebaseui": "^4.5.1",
    "grpc": "^1.22.2",
    "next": "latest",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-chartist": "^0.14.3",
    "react-dom": "^16.13.1",
    "react-image-gallery": "^1.0.7",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.25.2",
    "react-spinners": "^0.8.3",
    "react-swipeable-views": "^0.13.9",
    "react-with-firebase-auth": "^1.3.2",
    "sass": "^1.26.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^6.1.0",
    "rimraf": "^2.6.0"
  }
}
